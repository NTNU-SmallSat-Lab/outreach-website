{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Subtitle": {
      "type": "string"
    },
    "CoverImage": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "DatePublished": {
      "type": "date"
    },
    "Body": {
      "type": "blocks"
    },
    "author": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "admin::user"
    },
    "slug": {
      "type": "uid",
      "targetField": "Title"
    }
  }
}
